{
  "collectionName": "users",
  "info": {
    "displayName": "User",
    "singularName": "user",
    "pluralName": "users",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "users-permissions": {
      "advanced": {
        "defaultRole": "authenticated"
      }
    }
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "unique": true,
      "required": true
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "password",
      "minLength": 6
    },
    "resetPasswordToken": {
      "type": "string"
    },
    "confirmationToken": {
      "type": "string"
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role"
    },



    "subjects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subject.subject",
      "mappedBy": "author"
    },
    "ai_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ai-session.ai-session",
      "mappedBy": "user"
    },
    "assessments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::assessment.assessment",
      "mappedBy": "user"
    },
    "progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::progress.progress",
      "mappedBy": "user"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "user"
    }
  }
}
