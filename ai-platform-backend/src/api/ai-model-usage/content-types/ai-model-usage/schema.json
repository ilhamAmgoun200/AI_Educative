      "required": true,
      "maxLength": 255
    },
    "tokens_used": {
      "type": "integer"
    },
    "cost": {
      "type": "decimal",
      "precision": 10,
      "scale": 4
    },
    "duration_ms": {
      "type": "integer"
    },
    "interactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::interaction.interaction",
      "mappedBy": "ai_model_usage"
    }
  }
