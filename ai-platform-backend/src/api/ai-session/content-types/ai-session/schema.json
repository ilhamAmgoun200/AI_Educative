{
  "kind": "collectionType",
  "collectionName": "ai_sessions",
  "info": {
    "singularName": "ai-session",
    "pluralName": "ai-sessions",
    "displayName": "AI Session",
    "description": "AI tutoring sessions for lessons"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "ai_personality": {
      "type": "string",
      "maxLength": 255
    },
    "status": {
      "type": "string",
      "maxLength": 50
    },
    "feedback": {
      "type": "text"
    },
    "summary": {
      "type": "text"
    },
    "started_at": {
      "type": "datetime"
    },
    "ended_at": {
      "type": "datetime"
    },
    "lesson": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lesson.lesson",
      "inversedBy": "ai_sessions"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "ai_sessions"
    },
    "interactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::interaction.interaction",
      "mappedBy": "ai_session"
    },
    "transcripts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transcript.transcript",
      "mappedBy": "ai_session"
    }
  }
}
