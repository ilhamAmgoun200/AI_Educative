{
  "kind": "collectionType",
  "collectionName": "interactions",
  "info": {
    "singularName": "interaction",
    "pluralName": "interactions",
    "displayName": "Interaction",
    "description": "User-AI interactions during sessions"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "sender": {
      "type": "enumeration",
      "enum": [
        "user",
        "ai"
      ],
      "required": true
    },
    "message_type": {
      "type": "enumeration",
      "enum": [
        "text",
        "audio",
        "text+audio"
      ],
      "default": "text"
    },
    "content": {
      "type": "text"
    },
    "metadata": {
      "type": "json"
    },
    "ai_session": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ai-session.ai-session",
      "inversedBy": "interactions"
    },
    "ai_model_usage": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ai-model-usage.ai-model-usage",
      "inversedBy": "interactions"
    }
  }
}
